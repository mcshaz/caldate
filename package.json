{
  "name": "caldate",
  "version": "2.0.4",
  "description": "Calendar date for date-holidays",
  "keywords": [
    "calendar",
    "date"
  ],
  "homepage": "https://github.com/commenthol/caldate",
  "bugs": {
    "url": "https://github.com/commenthol/caldate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commenthol/caldate.git"
  },
  "license": "ISC",
  "author": "commenthol <commenthol@gmail.com>",
  "maintainers": [
    "commenthol <commenthol@gmail.com>"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "moduleResolution": "node",
  "types": "./dist",
  "directories": {
    "lib": "dist",
    "test": "test"
  },
  "scripts": {
    "all": "npm-run-all clean lint build test",
    "ci": "npm run all",
    "build": "npm-run-all bundle cjsCascade",
    "bundle": "tsup src/index.ts --dts --treeshake --format esm,cjs",
    "clean": "rimraf lib es coverage .nyc_output",
    "clean:all": "npm-run-all clean clean:node_modules",
    "clean:node_modules": "rimraf node_modules",
    "coverage": "c8 -r text -r html npm run test:ci",
    "lint": "eslint --ext .ts",
    "prepublishOnly": "npm run all",
    "readme": "markedpp --githubid -i README.md -o README.md",
    "test": "mocha",
    "cjsCascade": "replace-in-files --string 'date-fns-tz/esm/' --replacement 'date-fns-tz/' --no-glob dist/index.cjs"
  },
  "dependencies": {
    "date-fns-tz": "^1.3.7"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "c8": "^7.12.0",
    "date-fns": "^2.29.3",
    "dtslint": "^4.2.1",
    "eslint": "^8.31.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "replace-in-files-cli": "^2.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.3.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
